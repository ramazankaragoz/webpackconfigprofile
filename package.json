{
  "name": "my-app",
  "version": "0.1.0",
  "description": "Webpack config profile app",
  "author": "ramazan.karagoz.official@gmail.com",
  "license": "MIT",
  "main": "index.js",
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1"
  },
  "scripts": {
    "start": "webpack-dev-server  --port 8080 --mode=development --config config/webpack.config.js",
    "build": "rimraf ./lib & webpack --config config/webpack.config.js",
    "dev": "webpack-dev-server --port 8080 --mode=development --config config/webpack.dev.config.js --watch  --progress",
    "test": "webpack-dev-server  --port 9090 --mode=development --config config/webpack.test.config.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/node": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.8.3",
    "@babel/plugin-transform-object-assign": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-react": "^7.8.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^5.0.1",
    "file-loader": "^1.1.11",
    "postcss": "^8.1.10",
    "postcss-loader": "^4.1.0",
    "style-loader": "^2.0.0",
    "js-to-less-var-loader": "^1.0.4",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
